<?xml version="1.0" encoding="utf-8"?>
<project>
	<app title="My singing boyfriend" file="My-singing-boyfriend" main="Main" version="0.0.1" company="oldflag21" />

	<set name="BUILD_DIR" value="export" />

	<window width="1280" height="720" fps="" background="#000000" hardware="true" vsync="false" />
	<window if="html5" width="1280" height="720" resizable="true" />
	<window if="desktop" orientation="landscape" fullscreen="false" resizable="true" />

	<classpath name="source" />

	<assets path="assets" />

	<haxelib name="flixel" />
	<haxelib name="flixel-addons" />

	<haxedef name="FLX_NO_MOUSE" if="mobile" />
	<haxedef name="FLX_NO_KEYBOARD" if="mobile" />
	<haxedef name="FLX_NO_TOUCH" if="desktop" />
	<haxedef name="FLX_NO_DEBUG" unless="debug" />
	<haxedef name="js-es" value="6" />

	<icon path="ico/icon.png" size="16"/>
	<icon path="ico/icon.png" size="32"/>
	<icon path="ico/icon.png" size="64"/>
	<icon path="ico/icon.png"/>

</project>